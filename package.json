  "scripts": {
    "clean": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "TZ=UTC nest start",
    "start:server": "TZ=UTC SERVER_PROFILE=Admin nest start",
    "start:worker": "TZ=UTC SERVER_PROFILE=Worker nest start",
    "start:st-worker": "TZ=UTC SERVER_PROFILE=StrategyWorker nest start",
    "start:bt-worker": "TZ=UTC SERVER_PROFILE=BacktestWorker nest start",
    "start:dl-worker": "TZ=UTC SERVER_PROFILE=ExDataLoaderWorker nest start",
    "start:dev": "TZ=UTC nest start --watch",
    "start:dev:server": "TZ=UTC SERVER_PROFILE=Admin nest start --watch",
    "start:debug": "TZ=UTC nest start --debug --watch",
    "start:prod": "TZ=UTC node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "test:cov": "TZ=UTC jest --coverage",
    "test:e2e": "TZ=UTC jest --config ./test/jest-e2e.json",
    "orm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/db/data-source.ts",
    "orm:mg": "yarn orm migration:generate -p",
    "orm:mr": "yarn orm migration:run",
    "orm:rv": "yarn orm migration:revert",
    "clean-build": "yarn install; yarn clean; yarn build; yarn orm:mr",
    "run-ts": "ts-node --require tsconfig-paths/register",
    "new-token": "yarn run-ts src/scripts/new-token",
    "new-id": "yarn run-ts src/scripts/new-id"
  },
